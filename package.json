{
  "name": "next-js-boilerplate",
  "version": "3.66.5",
  "author": "Ixartz (https://github.com/ixartz)",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev:spotlight": "spotlight-sidecar",
    "dev:next": "next dev",
    "dev": "run-p dev:spotlight dev:next",
    "dev:test": "cross-env NEXT_PUBLIC_BYPASS_AUTH=true run-p dev:spotlight dev:next",
    "dev:auth": "cross-env NEXT_PUBLIC_BYPASS_AUTH=false run-p dev:spotlight dev:next",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -c production -- drizzle-kit migrate",
    "db:studio": "dotenv -c production -- drizzle-kit studio",
    "prepare": "husky"
  },
  "dependencies": {
    "@arcjet/next": "^1.0.0-beta.6",
    "@clerk/localizations": "^3.14.4",
    "@clerk/nextjs": "^6.18.5",
    "@electric-sql/pglite": "^0.3.0",
    "@hookform/resolvers": "^5.0.1",
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "@langchain/google-genai": "^0.2.7",
    "@logtail/pino": "^0.5.4",
    "@mynaui/icons-react": "^0.3.3",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^8.55.0",
    "@spotlightjs/spotlight": "^2.13.3",
    "@supabase-cache-helpers/postgrest-react-query": "^1.13.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "@t3-oss/env-nextjs": "^0.13.4",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-query": "^5.76.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.43.1",
    "immer": "^10.1.1",
    "langchain": "^0.3.24",
    "lucide-react": "^0.507.0",
    "mammoth": "^1.9.1",
    "next": "^15.3.1",
    "nuqs": "^2.4.3",
    "pdf-parse": "^1.1.1",
    "pg": "^8.15.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "posthog-js": "^1.239.1",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.2",
    "react-scroll-area": "^0.1.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "word-extractor": "^1.0.4",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@eslint-react/eslint-plugin": "^1.48.5",
    "@faker-js/faker": "^9.7.0",
    "@next/bundle-analyzer": "^15.3.1",
    "@next/eslint-plugin-next": "^15.3.1",
    "@percy/cli": "1.30.10",
    "@percy/playwright": "^1.0.8",
    "@playwright/test": "^1.52.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/postcss": "^4.1.6",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.11.14",
    "@types/react": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/expect": "^3.1.2",
    "checkly": "^5.3.1",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.26.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.1.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^4.1.6",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2",
    "vitest-fail-on-console": "^0.7.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "overrides": {
    "@mynaui/icons-react": {
      "react": "$react"
    }
  }
}
